<Window x:Class="PetStore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetStore"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="352,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Image x:Name="PetStoreLogo" HorizontalAlignment="Left" Height="71" Margin="10,10,0,0" VerticalAlignment="Top" Width="309" Source="/Pictures/PetStore Logo.png" AutomationProperties.Name="Pet Store" />
        <TextBlock HorizontalAlignment="Left" Margin="387,87,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="550,77,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Image x:Name="Dog" HorizontalAlignment="Left" Margin="67,170,0,0" VerticalAlignment="Top" Source="/Pictures/Dog.jfif" Height="73" Width="98"/>
        <Image x:Name="Cat" HorizontalAlignment="Left" Margin="216,154,0,0" VerticalAlignment="Top" Source="/Pictures/Cat.png" Height="104" Width="70"/>

    </Grid>
</Window>
